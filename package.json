{
  "name": "spyne-base-app",
  "version": "1.0.0",
  "description": "SpyneJS Base Application",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "webpack serve --open",
    "build": "webpack --mode production --env build",
    "build:apache": "buildType=apache webpack --mode production --env build",
    "installLinks": "npm link spyne spyne-plugin-scroll-lock  spyne-cli @franciscobatista/spyne-plugin-code-playground spyne-plugin-starter-template @franciscobatista/spyne-plugin-json-cms spyne-plugin-console",
    "test": "npx web-test-runner --config web-test-runner.config.js --log-level",
    "test:view": "junit-viewer --results=src/tests/results/unit-test-results.xml --save=src/tests/results/test-results.html",
    "saveImgsToLocal": "node ./save-imgs-to-local.js",
    "lint": "eslint --config eslint.config.js",
    "newL": "node ./repo-scripts/scripts.js",
    "cycleImages": "node ./repo-scripts/cycle-images.js",
    "cycleBizImages": "node ./repo-scripts/cycle-business-imgs.js",
    "lint:fix": "eslint --config eslint.config.js --fix",
    "migrate-sass": "sass-migrator module --migrate-deps src/scss/main-old.scss",
    "format": "prettier --write 'src/**/*.{js,css,scss,html}'"
  },
  "dependencies": {
    "dompurify": "^3.2.3",
    "ramda": "^0.30.1",
    "rxjs": "^7.8.1",
    "spyne": "^0.20.4"
  },
  "devDependencies": {
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@web/dev-server": "^0.4.6",
    "@web/dev-server-esbuild": "^1.0.3",
    "@web/test-runner": "^0.19.0",
    "@web/test-runner-commands": "^0.9.0",
    "@web/test-runner-junit-reporter": "^0.7.2",
    "@web/test-runner-mocha": "^0.9.0",
    "chai": "^5.1.2",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-mocha": "^10.5.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-webpack-plugin": "^4.2.0",
    "file-loader": "^6.2.0",
    "globals": "^15.14.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "jsdom": "^26.0.0",
    "karma": "^6.4.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-mocha": "^2.0.1",
    "karma-webpack": "^5.0.1",
    "mini-css-extract-plugin": "^2.9.2",
    "mocha": "^11.1.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.4.2",
    "requirejs": "^2.3.7",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.83.0",
    "sass-loader": "^16.0.4",
    "sass-migrator": "^2.3.0",
    "spyne-cli": "^0.6.0",
    "spyne-plugin-console": "^0.5.1",
    "style-loader": "^4.0.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "repository": {
    "type": "git",
